import numpy as np
import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt

# Load data set

Flower =sns.load_dataset("iris")

print (Flower) 

# draw a line plot

sns.barplot(x='species', y='sepal_width', data=Flower, order=['setosa','virginica', 'versicolor species'], color='red')

# Labeling

plt.title("bar plot")



plt.show


